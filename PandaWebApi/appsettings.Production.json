{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Https": {
        "Url": "https://*:443",
        "Protocols": "Http1AndHttp2AndHttp3",
        "Certificate": {
          "Path": "./Certificate/certificate.cer",
          "KeyPath": "./Certificate/certificate.key"
        }
      }
    }
  }
}