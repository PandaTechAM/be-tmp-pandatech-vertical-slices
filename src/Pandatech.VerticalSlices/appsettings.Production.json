{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Https": {
        "Url": "https://*:443",
        "Certificate": {
          "SslProtocols": [
            "Tls12",
            "Tls13"
          ],
          "Path": "/mnt/secrets/tls.crt",
          "KeyPath": "/mnt/secrets/tls.key"
        },
        "Protocols": "Http1AndHttp2AndHttp3"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "ResponseCrafterVisibility": "Public",
  "ConnectionStrings": {
    "Postgres": "**",
    "ElasticSearch": "**",
    "Redis": "**",
    "AuditTrail": "**"
  },
  "RabbitMqSettings": {
    "RabbitMqHost": "**",
    "AuditTrail": {
      "ExchangeName": "**",
      "RoutingKey": "**",
      "QueueName": "**",
      "RoutingKeyDlx": "**",
      "QueueNameDlx": "**"
    }
  },
  "Security": {
    "SuperUser": {
      "Username": "**",
      "Password": "**"
    },
    "Hangfire": {
      "Username": "**",
      "Password": "**"
    },
    "RefreshTokenExpirationMinutes": "**",
    "RefreshTokenMaxExpirationMinutes": "**",
    "CookieDomain": "**",
    "AESKey": "**",
    "Base36Chars": "**"
  },
  "ElasticIndexName": "**",
  "CorsSettings": {
    "AllowedOrigins": "**"
  }
}
